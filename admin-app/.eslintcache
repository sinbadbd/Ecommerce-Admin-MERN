/>\n    )\n}\n\nexport default SignUp","/Users/imran/Desktop/Front-End/Ecommerce-MERN/admin-app/src/Container/Home/index.js",["129","130","131","132"],"import React from 'react'\nimport { Col, Container, Row } from 'react-bootstrap'\nimport Layout from '../../Component/Layout'\nimport { NavLink } from 'react-router-dom';\n\nconst Home = (props) => {\n    return (\n        <>\n            <Layout sidebar>\n \n            </Layout>\n        </>\n    )\n}\n\nexport default Home\n",["133","134"],"/Users/imran/Desktop/Front-End/Ecommerce-MERN/admin-app/src/Component/UI/index.js",["135","136"],"/Users/imran/Desktop/Front-End/Ecommerce-MERN/admin-app/src/store/index.js",[],"/Users/imran/Desktop/Front-End/Ecommerce-MERN/admin-app/src/reducers/index.js",[],"/Users/imran/Desktop/Front-End/Ecommerce-MERN/admin-app/src/actions/constance.js",[],"/Users/imran/Desktop/Front-End/Ecommerce-MERN/admin-app/src/actions/auth.actions.js",[],"/Users/imran/Desktop/Front-End/Ecommerce-MERN/admin-app/src/actions/index.js",[],"/Users/imran/Desktop/Front-End/Ecommerce-MERN/admin-app/src/reducers/auth.reducers.js",["137","138","139"],"/Users/imran/Desktop/Front-End/Ecommerce-MERN/admin-app/src/helper/axios.js",[],"/Users/imran/Desktop/Front-End/Ecommerce-MERN/admin-app/src/Component/HOC/PrivateRoute.js",[],"/Users/imran/Desktop/Front-End/Ecommerce-MERN/admin-app/src/UrlConfig.js",[],["140","141"],"/Users/imran/Desktop/Front-End/Ecommerce-MERN/admin-app/src/reducers/user.reducers.js",["142","143"],"/Users/imran/Desktop/Front-End/Ecommerce-MERN/admin-app/src/Container/Product/product.js",["144","145","146"],"import React, { useEffect, useState } from \"react\";\nimport Layout from '../../Component/Layout'\nimport { Col, Container, Row, Modal, Button, Form } from \"react-bootstrap\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport Input from \"../../Component/UI/index\";\nimport {  addProduct } from \"../../actions\";\n\nconst Products = (props) => {\n\n    const [prouctName, se